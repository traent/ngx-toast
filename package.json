{
  "name": "ngx-toast",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "tw": "tailwindcss",
    "start": "ng serve",
    "build": "npm run build:lib",
    "watch": "ng build @traent/ngx-toast --watch --configuration development",
    "lint:lib": "ng lint --project @traent/ngx-toast",
    "tailwind:lib": "npm run tw -- -c projects/traent/ngx-toast/tailwind.config.js -i projects/traent/ngx-toast/assets/index.scss -o dist/traent/ngx-toast/assets/styles.min.scss --minify",
    "build:lib": "ng build --project @traent/ngx-toast --configuration production && npm run tailwind:lib",
    "serve:example": "ng serve --project example-app",
    "build:example": "ng build --project example-app --configuration production",
    "lint:example": "ng lint --project example-app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.0",
    "@traent/design-system": "^0.0.7",
    "rxjs": "~7.5.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "~14.2.9",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-plugin-import": "^2.27.5",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.2.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "~4.7.2"
  }
}
